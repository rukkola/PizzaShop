<h2>Your cart</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Price</th>
      <th scope="col">Qty</th>
    </tr>
  </thead>
  <tbody>

  <%  @items.each do |item| %>
	    <tr>
	      <th scope="row">1</th>
	      <td><%= item[0].title %></td>
	      <td><%= item[0].price %></td>
	      <td><%= item[1] %></td>
	    </tr>
    <% end %>

  </tbody>
</table>